set title description
set style data histogram
set style histogram cluster gap 1
set style fill solid border -1
set terminal pngcairo size 1000, 500
set xlabel "Serialization Formats"
set xtics rotate by 90 right
set xlabel offset 0,1
set ylabel "Byte Size"
set lmargin 9

plot filename using 3:xtic(2) title 'Uncompressed',\
filename using 4:xtic(2) title 'GZIP',\
filename using 5:xtic(2) title 'LZ4',\
filename using 6:xtic(2) title 'LZMA'
